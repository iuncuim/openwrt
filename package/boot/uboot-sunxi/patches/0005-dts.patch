From 5ed7621d8371e8de9340d0176f515ebfae2aa2df Mon Sep 17 00:00:00 2001
From: iuncuim <iuncuim@gmail.com>
Date: Thu, 9 Feb 2023 15:06:13 +0600
Subject: [PATCH] dts

---
 arch/arm/dts/sun50i-h616-orangepi-zero2.dts | 233 ++++++++++++++------
 arch/arm/dts/sun50i-h616.dtsi               |  10 +-
 2 files changed, 173 insertions(+), 70 deletions(-)

diff --git a/arch/arm/dts/sun50i-h616-orangepi-zero2.dts b/arch/arm/dts/sun50i-h616-orangepi-zero2.dts
index e6de49f89e..a86b3519b9 100644
--- a/arch/arm/dts/sun50i-h616-orangepi-zero2.dts
+++ b/arch/arm/dts/sun50i-h616-orangepi-zero2.dts
@@ -9,7 +9,6 @@
 
 #include <dt-bindings/gpio/gpio.h>
 #include <dt-bindings/interrupt-controller/arm-gic.h>
-#include <dt-bindings/leds/common.h>
 
 / {
 	model = "OrangePi Zero2";
@@ -17,6 +16,7 @@
 
 	aliases {
 		ethernet0 = &emac0;
+		ethernet1 = &emac1;
 		serial0 = &uart0;
 	};
 
@@ -27,18 +27,11 @@
 	leds {
 		compatible = "gpio-leds";
 
-		led-0 {
-			function = LED_FUNCTION_POWER;
-			color = <LED_COLOR_ID_RED>;
-			gpios = <&pio 2 12 GPIO_ACTIVE_HIGH>; /* PC12 */
+		power {
+			label = "orangepi:blue:power";
+			gpios = <&pio 4 19 GPIO_ACTIVE_LOW>; /* PE19 */
 			default-state = "on";
 		};
-
-		led-1 {
-			function = LED_FUNCTION_STATUS;
-			color = <LED_COLOR_ID_GREEN>;
-			gpios = <&pio 2 13 GPIO_ACTIVE_HIGH>; /* PC13 */
-		};
 	};
 
 	reg_vcc5v: vcc5v {
@@ -55,10 +48,15 @@
 		regulator-name = "usb1-vbus";
 		regulator-min-microvolt = <5000000>;
 		regulator-max-microvolt = <5000000>;
-		vin-supply = <&reg_vcc5v>;
-		enable-active-high;
-		gpio = <&pio 2 16 GPIO_ACTIVE_HIGH>; /* PC16 */
-		status = "okay";
+		regulator-always-on;
+	};
+	reg_vccio: vccio {
+		compatible = "regulator-fixed";
+		regulator-name = "vccio";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		gpio = <&pio 3 22 GPIO_ACTIVE_LOW>;
+		regulator-always-on;
 	};
 };
 
@@ -70,33 +68,111 @@
 	status = "okay";
 };
 
-/* USB 2 & 3 are on headers only. */
+&ehci2 {
+	status = "okay";
+};
+
+&ehci3 {
+	status = "okay";
+};
+
+&pio {
+	vcc-pa-supply = <&reg_dcdc1>;
+	vcc-pc-supply = <&reg_aldo1>;
+	vcc-pd-supply = <&reg_dcdc1>;
+	vcc-pe-supply = <&reg_aldo5>;
+	vcc-pf-supply = <&reg_dcdc1>;
+	vcc-pg-supply = <&reg_dcdc1>;
+	vcc-ph-supply = <&reg_dcdc1>;
+	vcc-pi-supply = <&reg_dcdc6>;
+
+	rgmii_phy_reset_pin: rgmii-phy-reset-pin {
+		pins = "PI6";
+		function = "gpio_out";
+	};
+
+	rmii_phy_reset_pin: rmii-phy-reset-pin {
+		pins = "PH5";
+		function = "gpio_out";
+	};
+};
 
 &emac0 {
 	pinctrl-names = "default";
-	pinctrl-0 = <&ext_rgmii_pins>;
+	pinctrl-0 = <&ext_rgmii_pins>, <&rgmii_phy_reset_pin>;
 	phy-mode = "rgmii";
 	phy-handle = <&ext_rgmii_phy>;
-	phy-supply = <&reg_dcdce>;
+	phy-supply = <&reg_dcdc6>;
+	phy-supply = <&reg_vccio>;
 	allwinner,rx-delay-ps = <3100>;
 	allwinner,tx-delay-ps = <700>;
+	
+	snps,reset-gpio = <&pio 8 6 GPIO_ACTIVE_HIGH>; /* PI6 */
+	//snps,reset-active-low;
+	snps,reset-delays-us = <0 10000 30000>;
+	
+	status = "okay";
+};
+
+&emac1 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&rmii_pins>, <&rmii_phy_reset_pin>;
+	phy-mode = "rmii";
+	phy-handle = <&rmii_phy>;
+	phy-supply = <&reg_dcdc6>;
+	allwinner,rx-delay-ps = <3100>;
+	allwinner,tx-delay-ps = <700>;
+	
+	snps,reset-gpio = <&pio 7 5 GPIO_ACTIVE_LOW>; /* PH5 */
+	//snps,reset-active-low;
+	snps,reset-delays-us = <0 10000 30000>;
+	
 	status = "okay";
 };
 
 &mdio0 {
-	ext_rgmii_phy: ethernet-phy@1 {
+	ext_rgmii_phy: ethernet-phy@6 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <6>;
+		/*
+		reset-gpios = <&pio 8 6 GPIO_ACTIVE_LOW>; // PI6
+		reset-assert-us = <50000>;
+		reset-deassert-us = <1000>;
+		*/
+	};
+};
+
+&mdio1 {
+	rmii_phy: ethernet-phy@1 {
 		compatible = "ethernet-phy-ieee802.3-c22";
 		reg = <1>;
+		/*
+		reset-gpios = <&pio 7 5 GPIO_ACTIVE_HIGH>; // PH5 
+		reset-assert-us = <50000>;
+		reset-deassert-us = <1000>;
+		*/
 	};
 };
 
 &mmc0 {
-	vmmc-supply = <&reg_dcdce>;
+	vmmc-supply = <&reg_dcdc1>;
 	cd-gpios = <&pio 5 6 GPIO_ACTIVE_LOW>;	/* PF6 */
 	bus-width = <4>;
 	status = "okay";
 };
 
+&mmc2 {
+	vmmc-supply = <&reg_dcdc1>;
+	vqmmc-supply = <&reg_aldo1>;
+	bus-width = <8>;
+	non-removable;
+	cap-mmc-highspeed;
+	max-frequency = <50000000>;
+	mmc-ddr-1_8v;
+	mmc-hs200-1_8v;
+	status = "okay";
+};
+
 &ohci0 {
 	status = "okay";
 };
@@ -105,22 +181,26 @@
 	status = "okay";
 };
 
-&r_rsb {
+
+&r_i2c {
 	status = "okay";
 
-	axp305: pmic@745 {
-		compatible = "x-powers,axp305", "x-powers,axp805",
-			     "x-powers,axp806";
+	axp305: pmic@36 {
+		compatible = "x-powers,axp858";
+		reg = <0x36>;
+
+		/* dummy interrupt to appease the driver for now */
+		interrupts = <GIC_SPI 103 IRQ_TYPE_LEVEL_HIGH>;
 		interrupt-controller;
 		#interrupt-cells = <1>;
-		reg = <0x745>;
 
 		x-powers,self-working-mode;
-		vina-supply = <&reg_vcc5v>;
-		vinb-supply = <&reg_vcc5v>;
-		vinc-supply = <&reg_vcc5v>;
-		vind-supply = <&reg_vcc5v>;
-		vine-supply = <&reg_vcc5v>;
+		vin1-supply = <&reg_vcc5v>;
+		vin2-supply = <&reg_vcc5v>;
+		vin3-supply = <&reg_vcc5v>;
+		vin4-supply = <&reg_vcc5v>;
+		vin5-supply = <&reg_vcc5v>;
+		vin6-supply = <&reg_vcc5v>;
 		aldoin-supply = <&reg_vcc5v>;
 		bldoin-supply = <&reg_vcc5v>;
 		cldoin-supply = <&reg_vcc5v>;
@@ -128,30 +208,41 @@
 		regulators {
 			reg_aldo1: aldo1 {
 				regulator-always-on;
-				regulator-min-microvolt = <3300000>;
-				regulator-max-microvolt = <3300000>;
-				regulator-name = "vcc-sys";
+				regulator-min-microvolt = <1800000>;
+				regulator-max-microvolt = <1800000>;
+				regulator-name = "vcc-aldo1";
 			};
 
-			reg_aldo2: aldo2 {	/* 3.3V on headers */
+			reg_aldo2: aldo2 {
 				regulator-always-on;
-				regulator-min-microvolt = <3300000>;
-				regulator-max-microvolt = <3300000>;
-				regulator-name = "vcc3v3-ext";
+				regulator-min-microvolt = <1800000>;
+				regulator-max-microvolt = <1800000>;
+				regulator-name = "vcc-aldo2";
 			};
 
-			reg_aldo3: aldo3 {	/* 3.3V on headers */
+			reg_aldo3: aldo3 {
 				regulator-always-on;
-				regulator-min-microvolt = <3300000>;
-				regulator-max-microvolt = <3300000>;
-				regulator-name = "vcc3v3-ext2";
+				regulator-min-microvolt = <1800000>;
+				regulator-max-microvolt = <1800000>;
+				regulator-name = "vcc-aldo3";
 			};
 
-			reg_bldo1: bldo1 {
+			reg_aldo4: aldo4 {
 				regulator-always-on;
 				regulator-min-microvolt = <1800000>;
 				regulator-max-microvolt = <1800000>;
-				regulator-name = "vcc1v8";
+				regulator-name = "vcc-aldo4";
+			};
+
+			reg_aldo5: aldo5 {
+				regulator-always-on;
+				regulator-min-microvolt = <3300000>;
+				regulator-max-microvolt = <3300000>;
+				regulator-name = "vcc-aldo5";
+			};
+
+			bldo1 {
+				/* unused */
 			};
 
 			bldo2 {
@@ -178,32 +269,46 @@
 				/* unused */
 			};
 
-			reg_dcdca: dcdca {
+			reg_dcdc1: dcdc1 {
+				regulator-always-on;
+				regulator-min-microvolt = <3300000>;
+				regulator-max-microvolt = <3300000>;
+				regulator-name = "vdd-dcdc1";
+			};
+
+			reg_dcdc2: dcdc2 {
 				regulator-always-on;
-				regulator-min-microvolt = <810000>;
-				regulator-max-microvolt = <1080000>;
+				regulator-min-microvolt = <900000>;
+				regulator-max-microvolt = <900000>;
 				regulator-name = "vdd-cpu";
 			};
 
-			reg_dcdcc: dcdcc {
+			reg_dcdc3: dcdc3 {
+				regulator-always-on;
+				regulator-min-microvolt = <900000>;
+				regulator-max-microvolt = <900000>;
+				regulator-name = "vdd-dcdc3";
+			};
+
+			reg_dcdc4: dcdc4 {
 				regulator-always-on;
-				regulator-min-microvolt = <810000>;
-				regulator-max-microvolt = <1080000>;
-				regulator-name = "vdd-gpu-sys";
+				regulator-min-microvolt = <900000>;
+				regulator-max-microvolt = <900000>;
+				regulator-name = "vdd-gpu";
 			};
 
-			reg_dcdcd: dcdcd {
+			reg_dcdc5: dcdc5 {
 				regulator-always-on;
-				regulator-min-microvolt = <1500000>;
-				regulator-max-microvolt = <1500000>;
+				regulator-min-microvolt = <1100000>;
+				regulator-max-microvolt = <1100000>;
 				regulator-name = "vdd-dram";
 			};
 
-			reg_dcdce: dcdce {
-				regulator-boot-on;
+			reg_dcdc6: dcdc6 {
+				regulator-always-on;
 				regulator-min-microvolt = <3300000>;
 				regulator-max-microvolt = <3300000>;
-				regulator-name = "vcc-eth-mmc";
+				regulator-name = "vdd-dcdc6";
 			};
 
 			sw {
@@ -213,18 +318,6 @@
 	};
 };
 
-&spi0  {
-	status = "okay";
-
-	flash@0 {
-		#address-cells = <1>;
-		#size-cells = <1>;
-		compatible = "jedec,spi-nor";
-		reg = <0>;
-		spi-max-frequency = <40000000>;
-	};
-};
-
 &uart0 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&uart0_ph_pins>;
@@ -232,11 +325,13 @@
 };
 
 &usbotg {
-	dr_mode = "peripheral";
+	dr_mode = "otg";
 	status = "okay";
 };
 
 &usbphy {
+	usb0_vbus-supply = <&reg_vcc5v>;
 	usb1_vbus-supply = <&reg_usb1_vbus>;
 	status = "okay";
 };
+ 
diff --git a/arch/arm/dts/sun50i-h616.dtsi b/arch/arm/dts/sun50i-h616.dtsi
index dd4d2f3111..d7ed654097 100644
--- a/arch/arm/dts/sun50i-h616.dtsi
+++ b/arch/arm/dts/sun50i-h616.dtsi
@@ -152,6 +152,13 @@
 				drive-strength = <40>;
 			};
 
+			rmii_pins: rmii-pins {
+				pins = "PA0", "PA1", "PA2", "PA3", "PA4",
+				       "PA5", "PA6", "PA7", "PA8", "PA9";
+				function = "emac1";
+				drive-strength = <40>;
+			};
+
 			i2c0_pins: i2c0-pins {
 				pins = "PI6", "PI7";
 				function = "i2c0";
@@ -476,7 +483,8 @@
 		};
 
 		emac1: ethernet@5030000 {
-			compatible = "allwinner,sun50i-h616-emac";
+			compatible = "allwinner,sun50i-h616-emac",
+				     "allwinner,sun50i-a64-emac";
 			syscon = <&syscon 1>;
 			reg = <0x05030000 0x10000>;
 			interrupts = <GIC_SPI 15 IRQ_TYPE_LEVEL_HIGH>;
