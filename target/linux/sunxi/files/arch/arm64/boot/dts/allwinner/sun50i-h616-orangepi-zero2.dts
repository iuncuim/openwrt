/dts-v1/;

#include "sun50i-h616.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/interrupt-controller/arm-gic.h>

/ {
	model = "MYiR MYB-YT507";
	compatible = "xunlong,orangepi-zero2", "allwinner,sun50i-h616";

	aliases {
		ethernet0 = &emac0;
		ethernet1 = &emac1;
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	leds {
		compatible = "gpio-leds";

		power {
			label = "orangepi:blue:power";
			gpios = <&pio 4 19 GPIO_ACTIVE_LOW>; /* PE19 */
			default-state = "on";
		};
	};

	reg_vcc5v: vcc5v {
		/* board wide 5V supply directly from the USB-C socket */
		compatible = "regulator-fixed";
		regulator-name = "vcc-5v";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-always-on;
	};

	reg_usb1_vbus: usb1-vbus {
		compatible = "regulator-fixed";
		regulator-name = "usb1-vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-always-on;
	};

	reg_gmac_3v3: gmac-3v3 {
		compatible = "regulator-fixed";
		regulator-name = "gmac-3v3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		//startup-delay-us = <100000>;
		enable-active-high;
		gpio = <&pio 3 22 GPIO_ACTIVE_HIGH>;
		regulator-always-on;
	};
};

&ehci0 {
	status = "okay";
};

&ehci1 {
	status = "okay";
};

&ehci2 {
	status = "okay";
};

&ehci3 {
	status = "okay";
};

&pio {
	vcc-pa-supply = <&reg_dcdc1>;
	vcc-pc-supply = <&reg_aldo1>;
	vcc-pd-supply = <&reg_aldo1>; // Don't know why
	vcc-pe-supply = <&reg_aldo5>;
	vcc-pf-supply = <&reg_dcdc1>;
	vcc-pg-supply = <&reg_dcdc1>;
	vcc-ph-supply = <&reg_dcdc1>;
	vcc-pi-supply = <&reg_dcdc6>;
	/*	
	rgmii_phy_reset_pin: rgmii-phy-reset-pin {
		pins = "PI6";
		function = "gpio_out";
	};
	rmii_phy_reset_pin: rmii-phy-reset-pin {
		pins = "PH5";
		function = "gpio_out";
	};
	*/
};

&emac0 {
	pinctrl-names = "default";
	pinctrl-0 = <&ext_rgmii_pins>;
	phy-mode = "rgmii-id";
	phy-handle = <&ext_rgmii_phy>;
	phy-supply = <&reg_dcdc6>,<&reg_gmac_3v3>;
	allwinner,rx-delay-ps = <3100>;
	allwinner,tx-delay-ps = <700>;
	status = "okay";
};

&emac1 {
	pinctrl-names = "default";
	pinctrl-0 = <&rmii_pins>;
	phy-mode = "rmii";
	phy-handle = <&rmii_phy>;
	phy-supply = <&reg_dcdc6>;
	allwinner,rx-delay-ps = <0>;
	allwinner,tx-delay-ps = <700>;
	//phy-reset-gpios = <&pio 7 5 GPIO_ACTIVE_HIGH>; // PH5
	//phy-reset-duration = <200>;
	//phy-reset-post-delay = <40>;
	status = "okay";
};

&mdio {
	ext_rgmii_phy: ethernet-phy@0 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0>;
		reset-gpios = <&pio 8 6 GPIO_ACTIVE_LOW>; // PI6
		reset-assert-us = <100000>;
		reset-deassert-us = <40000>;
		
	};
};

&mdio1 {
	/*
	reset-gpios = <&pio 7 5 GPIO_ACTIVE_HIGH>; // PH5
	reset-assert-us = <50000>;
	reset-deassert-us = <1000>;
	*/
	rmii_phy: ethernet-phy@0 {
		//clocks = <&ccu CLK_EMAC_25M>;
		//reset-gpios = <&pio 7 5 GPIO_ACTIVE_HIGH>; // PH5
		//reset-assert-us = <100000>;
		//reset-deassert-us = <40000>;
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0>;
	};
};

&mmc0 {
	vmmc-supply = <&reg_dcdc1>;
	cd-gpios = <&pio 5 6 GPIO_ACTIVE_LOW>;	/* PF6 */
	bus-width = <4>;
	status = "okay";
};

&mmc2 {
	vmmc-supply = <&reg_dcdc1>;
	vqmmc-supply = <&reg_aldo1>;
	broken-cd;
	bus-width = <8>;
	non-removable;
	cap-mmc-highspeed;
	max-frequency = <50000000>;
	mmc-ddr-1_8v;
	mmc-hs200-1_8v;
	status = "okay";
};

&ohci0 {
	status = "okay";
};

&ohci1 {
	status = "okay";
};


&r_i2c {
	status = "okay";

	axp305: pmic@36 {
		compatible = "x-powers,axp858";
		reg = <0x36>;

		/* dummy interrupt to appease the driver for now */
		interrupts = <GIC_SPI 103 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-controller;
		#interrupt-cells = <1>;

		x-powers,self-working-mode;
		vin1-supply = <&reg_vcc5v>;
		vin2-supply = <&reg_vcc5v>;
		vin3-supply = <&reg_vcc5v>;
		vin4-supply = <&reg_vcc5v>;
		vin5-supply = <&reg_vcc5v>;
		vin6-supply = <&reg_vcc5v>;
		aldoin-supply = <&reg_vcc5v>;
		bldoin-supply = <&reg_vcc5v>;
		cldoin-supply = <&reg_vcc5v>;

		regulators {
			reg_aldo1: aldo1 {
				regulator-always-on;
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-name = "vcc-aldo1";
			};

			reg_aldo2: aldo2 {
				regulator-always-on;
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-name = "vcc-aldo2";
			};

			reg_aldo3: aldo3 {
				regulator-always-on;
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-name = "vcc-aldo3";
			};

			reg_aldo4: aldo4 {
				regulator-always-on;
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-name = "vcc-aldo4";
			};

			reg_aldo5: aldo5 {
				regulator-always-on;
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-name = "vcc-aldo5";
			};

			bldo1 {
				/* unused */
			};

			bldo2 {
				/* unused */
			};

			bldo3 {
				/* unused */
			};

			bldo4 {
				/* unused */
			};

			cldo1 {
				/* reserved */
			};

			cldo2 {
				/* unused */
			};

			cldo3 {
				/* unused */
			};

			reg_dcdc1: dcdc1 {
				regulator-always-on;
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-name = "vdd-dcdc1";
			};

			reg_dcdc2: dcdc2 {
				regulator-always-on;
				regulator-min-microvolt = <900000>;
				regulator-max-microvolt = <900000>;
				regulator-name = "vdd-cpu";
			};

			reg_dcdc3: dcdc3 {
				regulator-always-on;
				regulator-min-microvolt = <900000>;
				regulator-max-microvolt = <900000>;
				regulator-name = "vdd-dcdc3";
			};

			reg_dcdc4: dcdc4 {
				regulator-always-on;
				regulator-min-microvolt = <900000>;
				regulator-max-microvolt = <900000>;
				regulator-name = "vdd-gpu";
			};

			reg_dcdc5: dcdc5 {
				regulator-always-on;
				regulator-min-microvolt = <1100000>;
				regulator-max-microvolt = <1100000>;
				regulator-name = "vdd-dram";
			};

			reg_dcdc6: dcdc6 {
				regulator-always-on;
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-name = "vdd-dcdc6";
			};

			sw {
				/* unused */
			};
		};
	};
};

&uart0 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart0_ph_pins>;
	status = "okay";
};

&usbotg {
	dr_mode = "otg";
	status = "okay";
};

&usbphy {
	usb0_vbus-supply = <&reg_vcc5v>;
	usb1_vbus-supply = <&reg_usb1_vbus>;
	status = "okay";
};
 
